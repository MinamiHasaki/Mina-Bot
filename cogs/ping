import discord
from discord.ext import commands


class Ping(commands.Cog):

    def __init__(self, client):
        self.client = client

    @commands.command()
    async def ping(self):
        await self.send("Pong! " + str(int(self.latency * 1000)) + "ms")
        print("Ping is loaded!")


def setup(client):
    client.add_cog(Ping(client))
